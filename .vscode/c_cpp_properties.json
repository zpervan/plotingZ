{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}/Plot/Common/src",
            "${workspaceFolder}/Plot/LinePlot/src",
            "${workspaceFolder}/Plot/Main/src",
            "${workspaceFolder}/Plot/ScatterPlot/src",
            "${workspaceFolder}/Plot/Utility/src",
            "${workspaceFolder}/ThirdParty/fmt/include",
            "${workspaceFolder}/ThirdParty/GoogleTest/googletest/include",
            "${workspaceFolder}/ThirdParty/GoogleTest/googlemock/include",
            "${workspaceFolder}/ThirdParty/SFML/include/SFML",
            "${workspaceFolder}/ThirdParty/SFML/include/SFML/**"
        ]
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        },
        {
            "name": "Win32",
            "compilerPath": "C:/Program Files (x86)/Microsoft Visual Studio/2019/Professional/VC/Tools/MSVC/14.29.30037/bin/Hostx64/x64/cl.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "msvc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "includePath": [
                "${default}",
                "${workspaceFolder}",
                "${workspaceFolder}/ThirdParty/GoogleTest/googletest/include"
            ]
        }
    ],
    "version": 4
}